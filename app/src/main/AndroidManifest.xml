        <activity android:name=".ajout_produit_intent" />
        <activity android:name=".update_client_intent" />
        <activity android:name=".ajout_client_intent" />
        <activity android:name=".commandIntent" />
        <activity android:name=".produitsIntent" />
        <activity android:name=".clientIntent" />
